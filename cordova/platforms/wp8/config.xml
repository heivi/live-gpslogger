<?xml version='1.0' encoding='utf-8'?>
<widget id="fi.virekunnas.kepardigps" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="wp-package" value="Device" />
    </feature>
    <feature name="BackgroundMode">
        <param name="wp-package" value="BackgroundMode" />
    </feature>
    <name>KeparDI GPS</name>
    <description>
    KeparDI live GPS-tracking
  </description>
    <author email="heikki@virekunnas.fi" href="http://heikki.virekunnas.fi">
    Heikki Virekunnas
  </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
